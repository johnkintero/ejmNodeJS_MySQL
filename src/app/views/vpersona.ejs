<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, user-scalable=no">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css">
  <title>Pits</title>
</head>

<body>
  <nav class="navbar navbar-dark bg-dark">
    <a href="#" class="navbar-brand">Pits</a>
  </nav>
  <div class="container">
    <div class="row">
      <div class="col-sm-4 mt-auto text-center">
        <table class="table table-responsive-sm table-sm w-auto">
          <thead>
            <tr>
              <th>ID</th>
              <th>documento</th>
              <th>nombre</th>
              <th>apellido</th>
              <th>activo</th>
              <th>fechaCreacion</th>
            </tr>
          </thead>
          <tbody>
            <% for(var i = 0; i< vdatos.length; i++) { %>
              <tr>
                <td>
                  <%= vdatos[i].IDPERSONA %>
                </td>
                <td>
                  <%= vdatos[i].STRNUMERODOCUMENTO %>
                </td>
                <td>
                  <%= vdatos[i].STRNOMBRE %>
                </td>
                <td>
                  <%= vdatos[i].STRAPELLIDO %>
                </td>
                <td>
                  <%= vdatos[i].BITACTIVO %>
                </td>
                <td>
                  <%= vdatos[i].DTMFECHACREACION %>
                </td>
              </tr>
              <% } %>
          </tbody>
        </table>
      </div>
    </div>
      <a href="/login">volver</a>
</body>
</html>
